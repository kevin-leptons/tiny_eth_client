{
  "name": "tiny_eth_client",
  "version": "0.3.0",
  "engines": {
    "node": "14"
  },
  "description": "Call ETH RPC by the right way",
  "main": "lib/index.js",
  "directories": {
    "doc": "doc",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "mocha --recursive --timeout 10000",
    "standardize": "trop_standard lib test",
    "doc": "jsdoc --destination docs --configure doc/jsdoc.json lib doc/jsdoc_homepage.md",
    "doc-server": "http-server docs",
    "doc-clean": "rm -rf docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevin-leptons/tiny_eth_client.git"
  },
  "keywords": [
    "ETH",
    "RPC",
    "API",
    "call"
  ],
  "author": "Kevin Leptons",
  "license": "See license",
  "bugs": {
    "url": "https://github.com/kevin-leptons/tiny_eth_client/issues"
  },
  "homepage": "https://github.com/kevin-leptons/tiny_eth_client#readme",
  "devDependencies": {
    "@trop/standard": "^0.8.0",
    "docdash": "^1.2.0",
    "http-server": "^14.0.0",
    "jsdoc": "^3.6.7",
    "minami": "^1.2.3",
    "mocha": "^9.1.3"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@trop/gear": "^0.4.2",
    "axios": "^0.24.0",
    "compute-gcd": "^1.2.1"
  }
}
